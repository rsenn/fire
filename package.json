{
  "name": "fire",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "assemblyscript": "^0.27.2",
    "assemblyscript-prettier": "^2.0.2",
    "prettier": "^3.0.0-alpha.6"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  },
  "scripts": {
    "asbuild:debug": "./node_modules/.bin/asc assembly/index.ts --target debug",
